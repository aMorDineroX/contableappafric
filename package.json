{
  "name": "contafricax",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.cjs",
    "build:vercel": "vite build --config vite.config.js",
    "build:vercel:old": "node build-vercel.js",
    "preview": "vite preview",
    "server": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.server.json src/server/index.ts",
    "server-win": "set NODE_ENV=development && ts-node-dev --project tsconfig.server.json --respawn --transpile-only --ignore-watch node_modules --no-notify --interval 2000 src/server/index.ts",
    "server-stable": "set NODE_ENV=development && ts-node --project tsconfig.server.json src/server/index.ts",
    "dev-stable": "vite --force --mode development --config vite.config.stable.ts",
    "start-win": "start-win-direct.bat"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^16.11.7",
    "@types/pg": "^8.6.6",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.17",
    "bcrypt": "^5.1.0",
    "chart.js": "^4.4.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "framer-motion": "^12.7.4",
    "jsonwebtoken": "^9.0.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.501.0",
    "nodemon": "^2.0.22",
    "pg": "^8.13.2",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.5.4",
    "xlsx": "^0.18.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "4.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/node": "^22.13.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.1.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.24.0",
    "vite": "^5.0.8"
  }
}
